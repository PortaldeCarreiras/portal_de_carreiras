// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Login {
  id            Int     @id @default(autoincrement())
  cpf           String  @unique
  senhaHash     String
  ativo         Boolean
  nome          String
  administrador Int

  @@map(name: "usuarios")
}
